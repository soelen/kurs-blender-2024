{
  "name": "kurs-blender-2024",
  "version": "1.0.0",
  "description": "A small presentation about Kurs Blender 2024",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && rollup --config rollup.config.js && node template.js && node workbox.mjs",
    "start": "concurrently \"nodemon -e js,md,css --watch src --watch README.md --exec npm run startagain\" \"npm run server\"",
    "startagain": "node template.js && rollup --config rollup.config.js",
    "server": "web-dev-server --app-index dist/index.html --root-dir dist/ --compatibility none --node-resolve --watch --open /",
    "qrcode": "qrcode -o assets/url.png \"https://kurs-blender-2024.netlify.app\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soelen/kurs-blender-2024.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://kurs-blender-2024.netlify.app",
  "devDependencies": {
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-strip": "^2.1.0",
    "@web/dev-server": "^0.1.29",
    "concurrently": "^7.0.0",
    "marked": "^4.0.12",
    "nodemon": "^2.0.15",
    "qrcode": "^1.5.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-copy": "^3.4.0",
    "workbox-build": "^6.4.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-precaching": "^6.4.2",
    "workbox-strategies": "^6.4.2"
  },
  "dependencies": {
    "hackers-tiny-slide-deck": "^0.3.1",
    "lit": "^2.1.2",
    "workbox-window": "^6.4.2"
  }
}
